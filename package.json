{
  "name": "team_boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-tabs": "^3.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production npm-run-all -s build _start_heroku_backend",
    "_start_heroku_backend": "node src/backend/app.js",
    "start-all": "npm-run-all -p start-frontend start-backend",
    "start-frontend": "react-scripts start",
    "start-backend": "nodemon src/backend/app.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
